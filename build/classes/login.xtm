<!DOCTYPE html>
<html lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Youtube Analytics</title>
    <link rel="icon" href="../favicon.png">
    <!-- Bootstrap -->
    <link href="../vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="../vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="../vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- Animate.css -->
    <link href="../vendors/animate.css/animate.min.css" rel="stylesheet">

    <!-- Custom Theme Style -->
    <link href="../build/css/custom.min.css" rel="stylesheet">
    
    
  </head>

  <body class="login">
    <div>
      <div class="box-waitting" style='text-align: center;height: 50px; display: none'><img style='height: 100%' src='../production/images/custom/loader.gif'></div>
      <input type="hidden" name="isDebug" id="isDebug" value="{{is_debug}}">
      <div class="login_wrapper">
        <div class="animate form login_form">
          <section class="login_content">
            <form>
              <h1>Login</h1>
              <div>
                <input type="text" class="form-control" placeholder="Username" required="" id="username-login" />
                <p class="text-danger"></p>
              </div>
              <div>
                
              </div>
              <div>
                <input type="password" class="form-control" placeholder="Password" required="" id="password-login"/>
                <p class="text-danger"></p>
              </div>
              
              <div id="login-error">
                <p class="text-danger"></p>
              </div>
              <div>
                <a class="btn btn-default submit" onclick="login();">Log in</a>
              </div>

              <div class="clearfix"></div>

              <div class="separator">
<!--                <p class="change_link">New to site?
                  <a href="#signup" class="to_register"> Create Account </a>
                </p>-->

                <div class="clearfix"></div>
                <br />

                <div>
                  <h1><i class="fa fa-area-chart"></i> Youtube Analytics</h1>
                  <p>Product By 1312614 - 1312335</p>
                </div>
              </div>
            </form>
          </section>
        </div>
      </div>
    </div>
      
    <script src="../vendors/jquery/dist/jquery.min.js" type="text/javascript"></script>
    <script src="../production/js/custom/loginPage.js" type="text/javascript"></script>
      
  </body>
</html>
